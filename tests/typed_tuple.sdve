transient  tuple {int, int} test = {0, 0};
state {a(0), b(1)} P1.state = 0;
int P1.x = 1;
int P1.y = 2;
state {a(0), b(1)} P2.state = 0;
int P2.z = 0;
int P2.w = 0;
transient bool t_0 = false;
transient bool t_1 = false;
transient bool t_2 = false;
transient bool t_3 = false;
transient bool t_4 = false;
transient bool t_5 = false;
transient bool t_6 = false;
transient bool t_7 = false;
transient bool t_8 = false;
	process P1
		guardBlock
			t_0 = P1.state == 1,
			t_1 = %t_0 and true;

		guardCondition t_1;
		effect
			P1.state = 1;

	process P2
		guardBlock
			t_2 = P2.state == 1,
			t_3 = %t_2 and true;

		guardCondition t_3;
		effect
			P2.state = 1;

	process P1-P2
		guardBlock
			t_4 = P1.state == 0,
			t_5 = t_4 and true,
			t_6 = P2.state == 0,
			t_7 = t_6 and true,
			t_8 = t_5 and t_7;

		guardCondition t_8;
		effect
			P1.state = 1,
			test = {P1.x, P1.y},
			P2.state = 1,
			P2.w = 2,
			{P2.w, P2.z} = test;
